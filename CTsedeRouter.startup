#####proxy configuration
/etc/init.d/snmpd start

#bloquear acessos ao serv http para ir p o proxy

iptables -A FORWARD -s 10.0.2.64/26 -d 10.0.0.131 -p tcp --dport 80 -j DROP
iptables -A FORWARD -s 10.0.0.128/26 -d 10.0.0.131 -p tcp --dport 80 -j DROP
iptables -A FORWARD -s 10.0.0.132 -d 10.0.0.131 -p tcp --dport 80 -j ACCEPT

#redirect to proxy
iptables -t nat -A PREROUTING -s 10.0.0.160/28 -p tcp --dport 80 -j DNAT --to-destination 10.0.0.132:3128
iptables -t nat -A PREROUTING -s 10.0.0.144/28 -p tcp --dport 80 -j DNAT --to-destination 10.0.0.132:3128
iptables -t nat -A PREROUTING -s 10.0.0.128/30 -p tcp --dport 80 -j DNAT --to-destination 10.0.0.132:3128




